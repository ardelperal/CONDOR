  "code": {
    "module": {
      "handlers": {
        "Form_Load": "Private Sub Form_Load()\n    ' Código de inicialización del formulario\n    Me.txtNombre.SetFocus\nEnd Sub",
        "Form_Current": "Private Sub Form_Current()\n    ' Código cuando cambia el registro actual\n    If Me.NewRecord Then\n        Me.Caption = \"Nuevo Registro\"\n    Else\n        Me.Caption = \"Editando: \" & Nz(Me.txtNombre, \"Sin nombre\")\n    End If\nEnd Sub",
        "Form_Close": "Private Sub Form_Close()\n    ' Código al cerrar el formulario\n    DoCmd.Save acForm, Me.Name\nEnd Sub",
        "txtNombre_Enter": "Private Sub txtNombre_Enter()\n    ' Código al entrar al campo nombre\n    Me.txtNombre.BackColor = RGB(255, 255, 200)\nEnd Sub",
        "txtNombre_Exit": "Private Sub txtNombre_Exit(Cancel As Integer)\n    ' Código al salir del campo nombre\n    Me.txtNombre.BackColor = RGB(255, 255, 255)\n    If Len(Trim(Me.txtNombre)) = 0 Then\n        MsgBox \"El nombre es requerido\", vbExclamation\n        Cancel = True\n    End If\nEnd Sub",
        "txtNombre_Change": "Private Sub txtNombre_Change()\n    ' Código cuando cambia el contenido del nombre\n    If Len(Me.txtNombre) > 50 Then\n        MsgBox \"El nombre no puede exceder 50 caracteres\", vbWarning\n    End If\nEnd Sub",
        "cmdGuardar_Click": "Private Sub cmdGuardar_Click()\n    ' Código para guardar el registro\n    On Error GoTo ErrorHandler\n    \n    If Me.Dirty Then\n        DoCmd.RunCommand acCmdSaveRecord\n        MsgBox \"Registro guardado exitosamente\", vbInformation\n    Else\n        MsgBox \"No hay cambios para guardar\", vbInformation\n    End If\n    \n    Exit Sub\n    \nErrorHandler:\n    MsgBox \"Error al guardar: \" & Err.Description, vbCritical\nEnd Sub"
      }
    }
  }
